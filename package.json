{
	"name": "sub-store-node",
	"version": "1.1.4",
	"private": true,
	"license": "GPL-3.0",
	"type": "module",
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev index.js --live-reload",
		"build": "node esbuild.js",
		"start": "node server.js",
		"server": "node server.js",
		"server:dev": "node --watch server.js",
		"pm2:start": "pm2 start ecosystem.config.cjs",
		"pm2:stop": "pm2 stop sub-store-node",
		"pm2:restart": "pm2 restart sub-store-node",
		"pm2:logs": "pm2 logs sub-store-node",
		"pm2:monit": "pm2 monit",
		"docker:build": "docker build -t sub-store-node .",
		"docker:run": "docker run -d -p 3000:3000 --name sub-store-node sub-store-node",
		"docker:stop": "docker stop sub-store-node && docker rm sub-store-node",
		"docker:compose:up": "docker-compose up -d",
		"docker:compose:down": "docker-compose down",
		"lint": "prettier --write \"**/*.js\""
	},
	"dependencies": {
		"express": "^4.21.2",
		"ip-address": "^10.0.1",
		"js-base64": "^3.7.8",
		"json5": "^2.2.3",
		"jsrsasign": "^11.1.0",
		"lodash": "^4.17.21",
		"peggy": "^5.0.6",
		"static-js-yaml": "^1.0.0"
	},
	"devDependencies": {
		"esbuild": "^0.25.9",
		"prettier": "^3.6.2",
		"wrangler": "^4.33.1"
	}
}